<p id="notice"><%= notice %></p>

<h1>Listing Calculos</h1>

<table>
  <thead>
    <tr>
      <th>Op1</th>
	  <th>Operador</th>
      <th>Op2</th>
	  <th>Medicion</th>
      <th>T0</th>
      <th>T1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calculos.each do |calculo| %>
      <tr>
        <td><center><%= calculo.op1 %></center></td>
		<td><center><%= calculo.operador %></center></td>
        <td><center><%= calculo.op2 %></center></td>
		<td><center><%= calculo.medicion %></center></td>
        <td><%= calculo.t0 %></td>
        <td><%= calculo.t1 %></td>
        <td><%= link_to 'Show', calculo %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calculo', new_calculo_path %>
